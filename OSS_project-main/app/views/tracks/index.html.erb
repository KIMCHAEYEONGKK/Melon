						<%break %>
					<%end %>
					<tr style="display: table-row;">
						<td style="height:84px; border-bottom:1px solid #f2f2f2; text-algin:left; vertical-algin:middle;">
							<div style = "text-algin: right; margin-top:31px;">
								
								<input type="checkbox" style="border: initial;background-color: initial;cursor: default;appearance: auto;box-sizing: border-box; vertical-algin:middle;" >
								
							</div>
						</td>
						<td style="text-align: left; vertical-align: middle; height:84px; border-bottom:1px solid #f2f2f2;">
							<div class="atist_id" style="text-align:center;">
								<span style="display: inline-block;padding-top: 5px;color: #333;font-size: 18px;width: 32px;padding: 0 18px 0 12px;" ><%= a %></span>
								<% a = a +1%>
								
							</div>
						</td>
						<!-- 순위 변동 -->
						<td style="border-bottom:1px solid #f2f2f2; text-algin:left; vertical-algin:middle;">
							<div class="wrap">
								<span style="width: 14px;height: 10px;background-position: 0 -15px;"></span>
							</div>
						</td>
						<td style="border-bottom:1px solid #f2f2f2; text-algin:left; vertical-algin:middle;">
							<div class="wrap">
								 <img style="width: 60px;aspect-ratio: auto 60 / 60;height: 60px;margin-top:10px; " src= "<%= track.album.album_link%>">
							</div>
						</td>
						<td style="border-bottom:1px solid #f2f2f2; text-algin:left; vertical-algin:middle;">
							<span style="width: 12px;background-position: -286px -30px;background-image: url(//cdnimg.melon.co.kr/resource/image/web/common/button_icons_sprite_20200521.png)-286px -30px;"> </span>
						</td>
						<td style="border-bottom:1px solid #f2f2f2;vertical-align: middle;">
							<div class="wrap" style="text-algin:left;">
