									<span style="color: #888;line-height: 14px;font-size: 12px;letter-spacing: 0;vertical-align: top;"><%= track.recommend %></span>
								</button>
								
							</div>
						</td>
						<td style="border-bottom: 1px solid #f2f2f2;text-align: left;vertical-align: middle;">
							<div style="text-align:center;">
								<%= audio_tag track.track_play, controls: "controls" , class: "my_audio",  style: "width: 19px;height: 28px;"%>
							</div>
